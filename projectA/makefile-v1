export sub.o=./lib/sub.o
export add.o=./lib/add.o
export libcal.a=./lib/libcal.a


all: main-v1
main-v1: ${sub.o} ${add.o} ${libcal.a}
	gcc -o main-v1 ./src/test.c -lcal -L ./lib -I ./include
${sub.o}: ./src/sub.c
	gcc -c ./src/sub.c -o ${sub.o}
${add.o}: ./src/add.c
	gcc -c ./src/add.c -o ${add.o}
${libcal.a}: ${sub.o} ${add.o}
	ar -rcs ${libcal.a} ${add.o} ${sub.o}
clean:
	rm -rf ./lib/*.o;
	rm -rf main-v1;
	rm -rf *.a


